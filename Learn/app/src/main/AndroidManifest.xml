<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.qwerty.learn" >

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/comein"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Activity.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.LvActivity"
            android:label="@string/title_activity_lv" >
        </activity>
        <activity
            android:name=".Activity.ChatActivity"
            android:label="@string/title_activity_chat" >
        </activity>
        <activity
            android:name=".Activity.SaveFileActivity"
            android:label="@string/title_activity_save_file" >
        </activity>
        <activity
            android:name=".Activity.MainActivity2Activity"
            android:label="@string/title_activity_main_activity2" >
        </activity>
        <activity
            android:name=".Activity.DatabaseActivity"
            android:label="@string/title_activity_database" >
        </activity>
        <activity
            android:name=".Activity.GetPerferencesActivity"
            android:label="@string/title_activity_get_perferences" >
        </activity>

        <service
            android:name=".Service.CalarmService"
            android:enabled="true"
            android:exported="true" >
        </service>
    </application>

</manifest>
